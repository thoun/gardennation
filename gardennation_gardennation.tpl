{OVERALL_GAME_HEADER}

<div id="score">
    <div id="tabble-wrapper">
        <table>
            <thead>
                <tr id="scoretr"></tr>
            </thead>
            <tbody id="score-table-body">
            </tbody>
        </table>
    </div>
</div>

<div id="zoom-wrapper">
    <div id="full-table">
        <div id="full-board-wrapper">
            <div id="full-board">
                <div id="board">
                    <div id="order-track">
                        <div id="order-track-0" class="order-track-number"></div>
                        <div id="order-track-1" class="order-track-number"></div>
                        <div id="order-track-2" class="order-track-number"></div>
                        <div id="order-track-3" class="order-track-number"></div>
                        <div id="order-track-4" class="order-track-number"></div>
                    </div>
                </div>
                <div id="common-projects">
                    <div id="remaining-roofs"></div>
                </div>
            </div>
        </div>

        <div id="playerstables"></div>
    </div>
    <div id="zoom-controls">
        <button id="zoom-out"></button>
        <button id="zoom-in" class="disabled"></button>
    </div>
</div>

{OVERALL_GAME_FOOTER}
